{
  "include": [
    "src",
    "assets/mockData.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "outDir": "build",
    "allowJs": false,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  }
}